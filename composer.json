{
    "name": "lsxiao/jwt-auth",
    "description": "JSON Web Token Authentication for Lumen",
    "authors": [
        {
            "name": "lsxiao",
            "email": "faith.epiphone@qq.com",
            "homepage": "https://www.zhihu.com/people/lsxiao"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/auth": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/http": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Lsxiao\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "keywords": [
        "jwt",
        "lumen",
        "laravel",
        "laravel5",
        "json web token"
    ],
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true
}